    "url": "https://github.com/doteco/www/issues"
  },
  "homepage": "https://github.com/doteco/www#readme",
  "dependencies": {
    "@sentry/browser": "6.13.3",
    "bootstrap": "3.4.1",
    "bootstrap-validator": "0.11.9",
    "jquery": "3.6.0"
  }
