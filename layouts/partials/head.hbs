<!DOCTYPE html>
{{#if faq}}
<html id="doteco" lang="{{lang}}" itemscope itemtype="https://schema.org/FAQPage">
{{else}}
<html id="doteco" lang="{{lang}}">
{{/if}}
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
	<meta name="description" content="{{description}}">
	<meta name="keywords" content="{{t "global:keywords" }}">
	{{#if author}}
	<meta name="author" content="{{author}}">
	{{/if}}

	{{#if noindex}}
	<meta name="robots" content="noindex,nofollow">
	{{/if}}

	<meta property="og:title" content="{{title}}">
	<meta property="og:description" content="{{description}}">
	<meta property="og:url" content="{{site-url}}/{{page-path}}">
	{{#if collection}}
	<meta property="og:type" content="article">
	<meta property="article:author" content="{{author}}">
	{{else}}
	<meta property="og:type" content="website">
	{{/if}}
	<meta property="og:image" content="{{site-url}}/img/{{og-image}}">
	<meta property="og:image:width" content="{{og-width}}">
	<meta property="og:image:height" content="{{og-height}}">
	{{#if header-img-alt}}
	<meta property="og:image:alt" content="{{header-img-alt}}">
	{{/if}}
	<meta name="twitter:card" content="summary_large_image">
	<meta name="twitter:site" content="{{twitter-id}}">
	<meta property="fb:app_id" content="744238272415478">
	{{#if pubdate}}
	<meta property="article:published_time" content="{{isoDate pubdate}}">
	{{/if}}

	<title>{{title}}</title>
	<link rel="canonical" href="{{site-url}}/{{page-path}}">
	<link rel="alternate" type="application/rss+xml" title=".eco news" href="/rss.xml">

	<!-- Fonts -->
	<link rel="preload" as="font" href="/css/fonts/lato-300.woff2" crossorigin="" type="font/woff2">
	<link rel="preload" as="font" href="/css/fonts/lato-700.woff2" crossorigin="" type="font/woff2">
	<link rel="preload" as="font" href="/css/fonts/opensans.woff2" crossorigin="" type="font/woff2">

	<link rel="stylesheet" href="/css/bootstrap/5.2.3/bootstrap.min.css">
	<link rel="stylesheet" href="/{{ fingerprint.[css/main.css] }}">
	<script defer src="/js/bootstrap/5.2.3/bootstrap.bundle.min.js"></script>

	<link rel="icon" href="/favicon.svg" type="image/svg+xml">
	<link rel="manifest" href="/site-v1.webmanifest">
	<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png">
	<link rel="mask-icon" href="/safari-pinned-tab.svg" color="#9F1744">
	<meta name="msapplication-TileColor" content="#9F1744">
	<meta name="theme-color" content="#ffffff">

	<script defer src="/js/sentry/6.19.7/bundle.min.js"></script>
	<script>
	document.addEventListener('DOMContentLoaded', function () {
		if (typeof Sentry !== 'undefined') {
			Sentry.init({
				dsn: "{{sentryDSN}}",
				allowUrls: [
					"{{site-url}}"
				]
			})
		}
	})
	</script>
	<script src="https://cdn.usefathom.com/script.js" data-site="ZKSAKYVJ" defer></script>

	<script type="application/ld+json">
	{
		"@context": "https://schema.org",
		"@type": "Organization",
		"name": ".eco",
		"legalName" : "Big Room Inc.",
		"url": "{{site-url}}",
		"logo": "{{site-url}}/img/doteco-logo.svg",
		"foundingDate": "2007",
		"founders": [{
			"@type": "Person",
			"name": "Trevor Bowden"
		}],
		"address": {
			"@type": "PostalAddress",
			"streetAddress": "1231 Pacific Boulevard, Suite 581",
			"addressLocality": "Vancouver",
			"addressRegion": "BC",
			"postalCode": "V6Z 0E2",
			"addressCountry": "Canada"
		},
	  "contactPoint": [{
	    "@type": "ContactPoint",
	    "email": "support@bigroom.eco",
			"telephone": "+1 604 682 6673",
	    "contactType": "customer service"
	  }],
	  "sameAs": [
			"https://en.wikipedia.org/wiki/.eco",
			"https://icannwiki.org/.eco",
			"https://www.linkedin.com/company/eco-domain/",
			"https://mastodon.eco/@doteco/",
			"https://twitter.com/doteco"
	  ]
	}
	</script>
	{{#if (equal lang 'en')}}
	<script type="application/ld+json">
	{
		"@context": "https://schema.org",
		"@type": "Product",
		"name": ".eco domain name",
		"image": "https://go.eco/img/doteco-logo.svg",
		"description": "Show your commitment to environmental action by registering your .eco domain name today. For businesses, organizations and individuals dedicated to helping our planet.",
		"brand": {
			"@type": "Brand",
			"name": ".eco domain registry"
		},
		"offers": {
			"@type": "AggregateOffer",
			"url": "https://go.eco/",
			"lowPrice": 60.00,
			"highPrice": 125.00,
			"priceCurrency": "USD",
			"offerCount": 8500,
			"availability": "https://schema.org/OnlineOnly"
		}
	}
	{{/if}}
	</script>
	{{#if collection}}
	<script type="application/ld+json">
	{
		"@context": "http://schema.org",
		"@type": "NewsArticle",
		"image": [
			"{{site-url}}{{header-img}}"
		],
		"url": "{{site-url}}/{{page-path}}",
		"datePublished": "{{isoDate pubdate}}",
		"dateModified": "{{isoDate update}}",
		"headline": "{{title}}",
		"description": "{{excerpt}}",
		"author": {
			"@type": "Organization",
			"name": ".eco",
			"url": "{{site-url}}"
		},
		"publisher": {
			"@type": "Organization",
			"name": ".eco Domain Registry",
			"url": "https://go.eco",
			"logo": {
				"@type": "ImageObject",
				"url": "{{site-url}}/img/doteco-logo.svg"
			}
		},
		"mainEntityOfPage": "{{site-url}}/{{page-path}}"
	}
	</script>
	{{/if}}
	{{#if backspace}}
	<script type="module" src="https://backspace.eco/b.js"></script>
	{{/if}}
</head>
<body{{#if active}} class="page-{{active}}"{{/if}}>
