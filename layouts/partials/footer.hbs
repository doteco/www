			<div class="row">

				{{#each (t "global:footer" returnObjects=true)}}
				<div class="col-xs-6 col-sm-3">
 					<ul>
 						<li><h5>{{head}}</h5></li>
						{{#each items}}
						<li>{{{this}}}</li>
