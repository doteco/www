---
title: '.eco whois'
description: 'Look up ownership of .eco domains'
page-path: whois/
og-image: opengraph/tiger-og.jpg
og-width: 1200
og-height: 630
active: whois
noindex: true
exclude: true
---

<div class="section whois">
 <div class="container">
   <h1 class="search-header">.eco whois</h1>
   <p class="subheader">Check who owns a .eco domain name.</p>

  
   <form method="get" class="search-form">
     <div class="row">
       <div class="col-sm-2"></div>
       <div class="col-sm-8">
        <div class="input-group">
          <input id="input-search" type="search" class="form-control domain-search" name="domain" placeholder="example.eco" autocomplete="off" autocapitalize="off" aria-describedby="domain-search-button">
          <button class="btn btn-search" type="submit" aria-label="{{t 'search.cta'}}" id="domain-search-button">
            <svg xmlns="http://www.w3.org/2000/svg" width="26" height="26" fill="currentColor" class="bi bi-search" viewBox="0 0 16 16">
              <path d="M11.742 10.344a6.5 6.5 0 1 0-1.397 1.398h-.001c.03.04.062.078.098.115l3.85 3.85a1 1 0 0 0 1.415-1.414l-3.85-3.85a1.007 1.007 0 0 0-.115-.1zM12 6.5a5.5 5.5 0 1 1-11 0 5.5 5.5 0 0 1 11 0z"/>
            </svg>
          </button>
        </div>
       </div>
       <div class="col-sm-2"></div>
     </div>
   </form>
 </div>
</div>

<div class="section">
  <pre class="container whois-results">
  </pre>
</div>

<script>
function whois () {
  const domainInput = document.querySelector('.domain-search')
  let domain = domainInput.value
  domain = domain.replace(/[\s;<>"'\/=()]/g, '').replace(/\..*$/, '')
  domain += '.eco'
  domainInput.value = domain

  if (window.history) {
    const url = new URL(window.location)
    url.searchParams.set('domain', domain)
    window.history.pushState({}, '', url)
  }

  window.fetch('{{ searchUrl }}/whois?domain=' + domain).then((response) => response.text()).then(function (results) {
    document.querySelector('.whois-results').innerText = results.replace(/"/g, '').replace(/\\r\\n/g, '\r\n')
  })
}

document.querySelector('.search-form').addEventListener('submit', function (e) {
  e.preventDefault()
  whois()
})

if ('URLSearchParams' in window) {
  const urlParams = new URLSearchParams(window.location.search)
  if (urlParams.has('domain')) {
    const domain = urlParams.get('domain')
    const domainInput = document.querySelector('.domain-search')
    domainInput.value = domain
    whois()
  }
}

</script>
